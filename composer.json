{
    "name": "advancedhosters/hidev-advancedhosters",
    "type": "yii2-extension",
    "description": "AdvancedHosters vendor configuration for HiDev",
    "keywords": [
        "advanced hosters",
        "hidev",
        "vendor config"
    ],
    "homepage": "https://github.com/advancedhosters/hidev-advancedhosters",
    "license": "proprietary",
    "support": {
        "email": "support@advancedhosters.com",
        "source": "https://github.com/advancedhosters/hidev-advancedhosters",
        "issues": "https://github.com/advancedhosters/hidev-advancedhosters/issues",
        "wiki": "https://github.com/advancedhosters/hidev-advancedhosters/wiki"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "email": "sol@hiqdev.com",
            "homepage": "https://hipanel.com/"
        },
        {
            "name": "Dmitry Naumenko",
            "email": "d.naumenko.a@gmail.com",
            "homepage": "http://silverfire.me/"
        }
    ],
    "require": {
        "yiisoft/composer-config-plugin": "dev-master",
        "opis/closure": "3.x-dev as 3.6.x-dev"
    },
    "require-dev": {
        "hiqdev/hidev-php": "dev-master",
        "hiqdev/hidev": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "advancedhosters\\hidev\\vendor\\": "src"
        }
    },
    "extra": {
        "config-plugin": {
            "hidev": "config/hidev.php"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
